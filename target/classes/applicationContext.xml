<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Bean definitions -->
    <bean id="emailService" class="com.revature.spring.di.EmailService"/>
    <bean id="smsService" class="com.revature.spring.di.SMSService"/>

    <bean id="notificationService" class="com.revature.spring.di.NotificationService">
        <property name="notificationService" ref="emailService"/>
    </bean>

</beans>